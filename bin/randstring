#!/usr/bin/perl -w

use Getopt::Std;
our( $opt_f );
getopts('f');
use strict;
if ( $opt_f ) {
	for ( 1 .. $ARGV[0]||8 ) { print chr( int( rand(94) + 33 ) ) } print "\n"
} else {
	my @a = ( "a" .. "z" , "A" .. "Z" , 0 .. 9 );
	for ( 1 .. $ARGV[0]||8 ) { print $a[int( rand(scalar @a ) ) ]} print "\n"
}
